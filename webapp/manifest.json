{
	"_version": "1.21.0",
	"sap.app": {
		"id": "app",
		"type": "application",
		"i18n": {
			"bundleUrl": "i18n/i18n.properties",
			"supportedLocales": [
				""
			],
			"fallbackLocale": ""
		},
		"title": "appTitle",
		"description": "appDescription",
		"applicationVersion": {
			"version": "1.0.0"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"resources": {
			"js": [
				{
					"url": "<url>"
				}
			],
			"css": [
				{
				  "uri": "css/style.css"
				}
			  ]
		},
		"rootView": {
			"viewName": "app.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.60",
			"libs": {
				"sap.m": {}
			}
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "app.i18n.i18n",
					"supportedLocales": [
						""
					],
					"fallbackLocale": ""
				}
			},
			"invoice": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "Invoices.json"
			},
			"shoppingCart": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "shoppingCart.json"
			},

			"wishList": {
				"type": "sap.ui.model.json.JSONModel",
				"uri": "wishList.json"
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "app.view",
				"controlId": "app",
				"controlAggregation": "detailPages",
				"transition": "filp",
				"async": true,
				"bypassed": {
					"target": [
						"Error"
					]
				}
			},
			"routes": [

				{
					"pattern": "",
					"name": "HomePage",
					"target": "home",
					"viewLevel": 1
				},

				{
					"pattern": "shop",
					"name": "SplitApp",
					"target": "master",
					"viewLevel": 2
				},

				{
					"pattern": "invoice/{invoiceId}",
					"name": "invoice",
					"target": ["detail","master"],
					"viewLevel": 2
				},

				{
					"pattern": "invoice/{invoiceId}/{productName}",
					"name": "invoiceInformaton",
					"target": ["detail","invoiceInformaton"],
					"viewLevel": 2
				},

				{
					"pattern": "shoppingCart",
					"name": "shoppingCart",
					"target": "shoppingcart",
					"viewLevel": 2
				},

				{
					"pattern": "wishList",
					"name": "wishList",
					"target": "wishList",
					"viewLevel": 2
				}
			],
			"targets": {

				"home": {
					"viewId": "homeId",
					"viewName": "Home",
					"controllAggregation": "pages"
				},

				"detail": {
					"viewId": "detailId",
					"viewName": "Detail",
					"viewPath": "app.view.productList",
					"controllAggregation": "detailPages"
				},


				"master": {
					"viewId": "masterId",
					"viewName": "Master",
					"viewPath": "app.view.productList",
					"controlAggregation": "masterPages"
				},

				"invoiceInformaton": {
					"viewId": "informationId",
					"viewName": "InvoiceInformation",
					"viewPath": "app.view.productList",
					"controlAggregation": "detailPages",
					"viewLevel" : 2,
					"transition": "flip"
				},

				"shoppingcart": {
					"viewId": "shoppingCartId",
					"viewName": "ShoppongCart",
					"controllAggregation": "pages"
				},

				"wishList": {
					"viewId": "wishListId",
					"viewName": "WishList",
					"viewLevel" : 3,
					"transition": "slide"
				}
			}
		}
	}
}